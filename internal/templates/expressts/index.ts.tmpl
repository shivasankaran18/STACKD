
import express  from 'express';
import  cors  from 'cors';

const app = express();
const port = 3000;

app.use(express.json());
app.use(cors());

app.get('/', (req:any, res:any) => {
    res.send('Hello from the server!');
});

app.listen(port, () => {
    console.log(`Server running at http://localhost:${port}`);
});
